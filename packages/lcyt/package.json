{
  "name": "lcyt",
  "version": "2.1.1",
  "description": "Send live captions to YouTube Live streams via HTTP POST endpoint",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./src/sender.js",
  "types": "./src/sender.d.ts",
  "exports": {
    ".": {
      "types": "./src/sender.d.ts",
      "import": "./src/sender.js",
      "require": "./dist/index.cjs"
    },
    "./errors": {
      "types": "./src/errors.d.ts",
      "import": "./src/errors.js",
      "require": "./dist/errors.cjs"
    },
    "./config": {
      "types": "./src/config.d.ts",
      "import": "./src/config.js",
      "require": "./dist/config.cjs"
    },
    "./logger": {
      "types": "./src/logger.d.ts",
      "import": "./src/logger.js",
      "require": "./dist/logger.cjs"
    },
    "./backend": {
      "types": "./src/backend-sender.d.ts",
      "import": "./src/backend-sender.js",
      "require": "./dist/backend-sender.cjs"
    }
  },
  "scripts": {
    "test": "node --test test/*.test.js",
    "build": "node scripts/build-cjs.js",
    "prepack": "node scripts/build-cjs.js"
  },
  "keywords": [
    "youtube",
    "live",
    "captions",
    "subtitles",
    "streaming",
    "closed-captions",
    "accessibility",
    "live-stream"
  ],
  "author": "Juha It√§leino <jsilvanus@gmail.com>",
  "license": "MIT",
  "files": [
    "src/",
    "dist/"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jsilvanus/live-captions-yt.git",
    "directory": "packages/lcyt"
  },
  "bugs": {
    "url": "https://github.com/jsilvanus/live-captions-yt/issues"
  },
  "homepage": "https://github.com/jsilvanus/live-captions-yt#readme"
}
