{
  "name": "lcyt",
  "version": "1.2.0",
  "description": "Send live captions to YouTube Live streams",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./src/sender.js",
  "exports": {
    ".": {
      "import": "./src/sender.js",
      "require": "./dist/index.cjs"
    },
    "./errors": {
      "import": "./src/errors.js",
      "require": "./dist/errors.cjs"
    }
  },
  "bin": {
    "lcyt": "bin/lcyt"
  },
  "scripts": {
    "start": "node bin/lcyt",
    "test": "node --test test/*.test.js",
    "build": "node scripts/build-cjs.js"
  },
  "keywords": [
    "youtube",
    "live",
    "captions",
    "subtitles",
    "streaming",
    "closed-captions",
    "accessibility",
    "live-stream",
    "cli"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "yargs": "^17.7.2"
  },
  "files": [
    "bin/",
    "src/",
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jsilvanus/live-captions-yt.git"
  },
  "bugs": {
    "url": "https://github.com/jsilvanus/live-captions-yt/issues"
  },
  "homepage": "https://github.com/jsilvanus/live-captions-yt#readme"
}
